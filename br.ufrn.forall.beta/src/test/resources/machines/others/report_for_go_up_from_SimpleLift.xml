<?xml version="1.0" encoding="UTF-8"?>
<test-suite>
    <machine-name>SimpleLift</machine-name>
    <machine-invariant>
        <invariant-clause>floor : 1..5</invariant-clause>
    </machine-invariant>
    <operation-under-test>go_up</operation-under-test>
    <partition-strategy>Equivalence Classes</partition-strategy>
    <combinatorial-criteria>Each-Choice</combinatorial-criteria>
    <oracle-strategy>
        <state-variables>true</state-variables>
        <return-variables>true</return-variables>
        <invariant-ok>true</invariant-ok>
    </oracle-strategy>
    <test-cases>
        <test-case>
            <id>1</id>
            <formula>floor &lt; 5 &amp; floor : 1..5</formula>
            <isNegative>false</isNegative>
            <state-variables>
                <variable>
                    <identifier>floor</identifier>
                    <values>
                        <value>1</value>
                    </values>
                </variable>
            </state-variables>
            <operation-parameters/>
            <expected-state-values>
                <variable>
                    <identifier>floor</identifier>
                    <values>
                        <value>2</value>
                    </values>
                </variable>
            </expected-state-values>
            <return-variables/>
        </test-case>
        <test-case>
            <id>2</id>
            <formula>not(floor &lt; 5) &amp; floor : 1..5</formula>
            <isNegative>true</isNegative>
            <state-variables>
                <variable>
                    <identifier>floor</identifier>
                    <values>
                        <value>5</value>
                    </values>
                </variable>
            </state-variables>
            <operation-parameters/>
            <expected-state-values>
                <variable>
                    <identifier>floor</identifier>
                    <values>
                        <value>unknown</value>
                    </values>
                </variable>
            </expected-state-values>
            <return-variables/>
        </test-case>
    </test-cases>
</test-suite>
