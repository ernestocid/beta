<?xml version="1.0" encoding="UTF-8"?>
<test-suite>
    <machine-name>Course</machine-name>
    <machine-invariant>
        <invariant-clause>grades : (students +-&gt; 0..5)</invariant-clause>
        <invariant-clause>has_taken_lab_classes : (students +-&gt; BOOL)</invariant-clause>
        <invariant-clause>students &lt;: all_students</invariant-clause>
    </machine-invariant>
    <operation-under-test>student_pass_or_fail</operation-under-test>
    <testing-strategy>Logical Coverage</testing-strategy>
    <coverage-criteria>Clause Coverage</coverage-criteria>
    <oracle-strategy>
        <state-variables>true</state-variables>
        <return-variables>true</return-variables>
        <invariant-ok>true</invariant-ok>
    </oracle-strategy>
    <test-cases/>
</test-suite>
