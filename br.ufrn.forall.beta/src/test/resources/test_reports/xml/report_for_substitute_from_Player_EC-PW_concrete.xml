<?xml version="1.0" encoding="UTF-8"?>
<test-suite>
    <machine-name>Player</machine-name>
    <machine-invariant>
        <invariant-clause>card(team) = 11</invariant-clause>
        <invariant-clause>team &lt;: PLAYER</invariant-clause>
    </machine-invariant>
    <operation-under-test>substitute</operation-under-test>
    <testing-strategy>Input Space Partition</testing-strategy>
    <coverage-criteria>Equivalent Classes (Pairwise)</coverage-criteria>
    <oracle-strategy>
        <state-variables>true</state-variables>
        <return-variables>true</return-variables>
        <invariant-ok>true</invariant-ok>
    </oracle-strategy>
    <test-cases>
        <test-case>
            <id>1</id>
            <existential-formula>#rr, pp, team.(not(pp : team) &amp; team &lt;: PLAYER &amp; not(rr /: team) &amp; rr : PLAYER &amp; pp : PLAYER &amp; card(team) = 11)</existential-formula>
            <formula>not(pp : team) &amp; team &lt;: PLAYER &amp; not(rr /: team) &amp; rr : PLAYER &amp; pp : PLAYER &amp; card(team) = 11</formula>
            <preamble/>
            <isNegative>true</isNegative>
            <state-variables>
                <variable>
                    <identifier>team</identifier>
                    <values>
                        <value>PLAYER1</value>
                        <value>PLAYER3</value>
                        <value>PLAYER4</value>
                        <value>PLAYER5</value>
                        <value>PLAYER6</value>
                        <value>PLAYER7</value>
                        <value>PLAYER8</value>
                        <value>PLAYER9</value>
                        <value>PLAYER10</value>
                        <value>PLAYER11</value>
                        <value>PLAYER12</value>
                    </values>
                </variable>
            </state-variables>
            <operation-parameters>
                <parameter>
                    <identifier>rr</identifier>
                    <values>
                        <value>PLAYER1</value>
                    </values>
                </parameter>
                <parameter>
                    <identifier>pp</identifier>
                    <values>
                        <value>PLAYER2</value>
                    </values>
                </parameter>
            </operation-parameters>
            <data-concretization>
                <input-variable>
                    <variable-name>rr</variable-name>
                    <variable-value>1</variable-value>
                </input-variable>
                <input-variable>
                    <variable-name>team_array</variable-name>
                    <variable-value>{(0|-&gt;1),(1|-&gt;9),(2|-&gt;2),(3|-&gt;11),(4|-&gt;3),(5|-&gt;4),(6|-&gt;8),(7|-&gt;6),(8|-&gt;7),(9|-&gt;5),(10|-&gt;10)}</variable-value>
                </input-variable>
                <input-variable>
                    <variable-name>pp</variable-name>
                    <variable-value>12</variable-value>
                </input-variable>
            </data-concretization>
        </test-case>
